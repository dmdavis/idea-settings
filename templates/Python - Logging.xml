<templateSet group="Python - Logging">
  <template name="info" value="$logging$.info($END$)" description="Add a logging.info statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="" defaultValue="&quot;self._log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="crit" value="$logging$.critical($END$)" description="Add a logging.critical statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="" defaultValue="&quot;self._log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="deb" value="$logging$.debug($END$)" description="Add a logging.debug statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="complete()" defaultValue="&quot;log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="fatal" value="$logging$.fatal($END$)" description="Add a logging.fatal statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="" defaultValue="&quot;self._log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="except" value="$logging$.exception($END$)" description="Add a logging.exception statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="" defaultValue="&quot;self._log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="error" value="$logging$.error($END$)" description="Add a logging.error statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="" defaultValue="&quot;self._log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="warn" value="$logging$.warn($END$)" description="Add a logging.warn statement." toReformat="false" toShortenFQNames="true">
    <variable name="logging" expression="" defaultValue="&quot;self._log&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="clog" value="self._log = logging.getLogger(type(self).__name__)" description="Create a logger for a new-style base class" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="bcl" value="logging.basicConfig(stream=sys.stdout, level=logging.DEBUG, datefmt='%H:%M:%S',&#10;                    format='%(asctime)s,%(msecs)d %(name)s %(levelname)s %(message)s')" description="Basic console logging" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="sinfo" value="log.msg(&quot;$message$&quot;, level=log.INFO)$END$" description="Add a Scrapy INFO log.msg statement" toReformat="false" toShortenFQNames="true">
    <variable name="message" expression="" defaultValue="&quot;message&quot;" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>